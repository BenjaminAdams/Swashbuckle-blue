<td width='15%' class='code'>{{code}} {{#unless noCopy}}<span class="copyw3" title="Copy the W3 http code specification to your clipboard">copy</span>{{/unless}} </td>
<td class="markdown">{{{message}}}</td>
<td width='50%'><span class="model-signature" /></td>
<td class="headers">
    <table>
        <tbody>
            {{#each headers}}
            <tr>
                <td>{{@key}}</td>
                <td>{{this.description}}</td>
                <td>{{this.type}}</td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</td>